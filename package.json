{
  "name": "@bibleio/bibleio",
  "version": "0.0.0",
  "author": "dukc",
  "license": "MIT-0",
  "packageManager": "pnpm@9.12.3",
  "scripts": {
    "site": "turbo run --filter=@bibleio/api",
    "site:dev": "turbo run dev --filter=@bibleio/website",
    "site:build": "turbo run build --filter=@bibleio/website",
    "webapp": "turbo run --filter=@bibleio/api",
    "webapp:dev": "turbo run dev --filter=@bibleio/web-app",
    "webapp:build": "turbo run build --filter=@bibleio/web-app",
    "docs": "turbo run --filter=@bibleio/api",
    "docs:dev": "turbo run dev --filter=@bibleio/docs",
    "docs:build": "turbo run build --filter=@bibleio/docs",
    "api": "turbo run --filter=@bibleio/api",
    "api:dev": "turbo run dev --filter=@bibleio/api",
    "api:build": "turbo run build --filter=@bibleio/api",
    "design": "turbo run --filter=@bibleio/design",
    "design:dev": "turbo run dev --filter=@bibleio/design",
    "fix": "turbo run fix"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "turbo": "^2.3.0"
  }
}
