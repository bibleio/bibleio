{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "install": {
      "cache": false
    },
    "build": {
      "outputs": ["dist/**"],
      "cache": true
    },
    "fix": {
      "outputs": []
    },
    "dev": {
      "persistent": true,
      "cache": false
    },
    "wrangler:dev": {
      "persistent": true,
      "cache": false,
      "dependsOn": ["build"]
    },
    "wrangler:dev:persist": {
      "persistent": true,
      "cache": false,
      "dependsOn": ["build"]
    },
    "dev:cf": {
      "persistent": true,
      "cache": false,
      "dependsOn": ["build"]
    }
  }
}
