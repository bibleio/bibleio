{
  "name": "bibleio-webapp",
  "main": "dist/_worker.js",
  "compatibility_date": "2024-05-24",

  // Enables Node.js APIs that your React/Astro code might need
  "compatibility_flags": ["nodejs_compat"],

  "assets": {
    "directory": "./dist/_astro",
    "binding": "ASSETS",
  },

  "vars": {
    "NODE_ENV": "production",
  },
  "env": {
    "development": {
      "name": "bibleio-webapp-dev",
      "vars": {
        "NODE_ENV": "development",
      },
    },
  },
}
